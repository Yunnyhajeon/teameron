<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2699FB">

    <Button
        android:id="@+id/screenings_btn"
        android:layout_width="150dp"
        android:layout_height="117dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="452dp"
        android:layout_marginEnd="35dp"
        android:layout_marginRight="35dp"
        android:text="Recommended Screenings" />

    <Button
        android:id="@+id/child_temp_btn"
        android:layout_width="150dp"
        android:layout_height="117dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="35dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="452dp"
        android:text="How to Take a Child's Temperature" />

    <Button
        android:id="@+id/test_btn"
        android:layout_width="150dp"
        android:layout_height="117dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="275dp"
        android:layout_marginEnd="35dp"
        android:layout_marginRight="35dp"
        android:text="Self Testicular Exam" />

    <TextView
        android:id="@+id/med_title"
        android:layout_width="301dp"
        android:layout_height="94dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_centerInParent="true"
        android:layout_centerVertical="false"
        android:layout_marginStart="55dp"
        android:layout_marginLeft="55dp"
        android:layout_marginTop="99dp"
        android:layout_marginEnd="55dp"
        android:layout_marginRight="55dp"
        android:text="Medical Guide Pages"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/home_btn_from_medical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back "
        android:textColor="#000000" />

    <Button
        android:id="@+id/breast_test_btn"
        android:layout_width="150dp"
        android:layout_height="117dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="35dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="275dp"
        android:text="Self Breast Examination" />

</RelativeLayout>